# Database Configuration
POSTGRES_DB=automata_db
POSTGRES_USER=automata
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Supabase Configuration
SUPABASE_POSTGRES_PASSWORD=your_secure_supabase_password_here
GOTRUE_SITE_URL=http://localhost:3000
GOTRUE_URI_ALLOW_LIST=*
GOTRUE_JWT_SECRET=your-super-secret-jwt-token-with-minimum-256-bits

# Backend Configuration
SECRET_KEY=your-backend-secret-key-minimum-256-bits
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
OLLAMA_BASE_URL=http://host.docker.internal:11434

# Frontend Configuration
VITE_API_URL=http://localhost:8000
VITE_SUPABASE_URL=http://localhost:9999
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Production URLs (for docker-compose.prod.yml)
FRONTEND_URL=https://your-domain.com
API_URL=https://api.your-domain.com
WS_URL=wss://api.your-domain.com

# Monitoring (Production)
PROMETHEUS_METRICS=true
HEALTH_CHECK_INTERVAL=30

# Error Monitoring
SENTRY_DSN=https://your-sentry-dsn-here
SENTRY_ENVIRONMENT=development

# Performance
REDIS_CACHE_TTL=3600
DATABASE_POOL_SIZE=20
DATABASE_MAX_OVERFLOW=30