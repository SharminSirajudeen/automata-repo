# Production Environment Variables
# Copy this file and set actual values for production deployment

# API Configuration
VITE_API_URL=https://api.automata-learning.com
VITE_WS_URL=wss://api.automata-learning.com

# Supabase Configuration (if using)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# Error Monitoring
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=production
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAYS_SESSION_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0

# Analytics (optional)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_PREMIUM_FEATURES=true

# Security
VITE_ENABLE_CSP=true
VITE_CSP_REPORT_URI=https://api.automata-learning.com/csp-report

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_PREFETCH=true
VITE_ENABLE_WEBP=true

# Debug (set to false in production)
VITE_DEBUG=false
VITE_SHOW_PERFORMANCE_METRICS=false